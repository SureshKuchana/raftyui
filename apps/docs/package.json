{
  "name": "@rhinobase/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run watch:*\"",
    "watch:css": "npx tailwindcss -i ./styles/globals.css -o ./public/globals.css --watch",
    "watch:storybook": "start-storybook dev -p 6011",
    "build": "build-storybook --docs",
    "preview-storybook": "serve storybook-static",
    "clean": "rm -rf .turbo && rm -rf node_modules"
  },
  "dependencies": {
    "@rhinobase/core": "*",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rhinobase/tsconfig": "*",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.0",
    "@storybook/react": "^6.5.16",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint-config-acme": "*",
    "serve": "^14.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  }
}